{
  "name": "nextjs-approute-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "dev:turbo": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "prepare": "husky",
    "analyze": "ANALYZE=true next build",
    "lighthouse": "lhci autorun",
    "lighthouse:local": "mkdir -p reports && lighthouse http://localhost:3000 --output=html --output-path=./reports/lighthouse.html --chrome-flags='--headless --no-sandbox'",
    "perf:audit": "node scripts/performance-audit.mjs",
    "perf:build": "ANALYZE=true next build && node scripts/performance-audit.mjs",
    "load-test": "k6 run scripts/load-test.k6.js",
    "load-test:local": "BASE_URL=http://localhost:3000 k6 run scripts/load-test.k6.js",
    "load-test:quick": "k6 run --vus 10 --duration 30s scripts/load-test.k6.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@next/third-parties": "^15.5.2",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.1.2",
    "@react-email/components": "^0.3.2",
    "@react-email/render": "^1.1.3",
    "@tanstack/react-query": "^5.71.1",
    "@tanstack/react-query-devtools": "^5.71.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^5.18.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "ajv": "^8.17.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.484.0",
    "next": "16.0.7",
    "next-swagger-doc": "^0.4.1",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.63.0",
    "react-icons": "^5.5.0",
    "resend": "^4.7.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.2.4",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^16.0.10",
    "@tailwindcss/postcss": "^4",
    "@tanstack/eslint-plugin-query": "^5.68.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lighthouse": "^13.0.1",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vitest": "^2.1.8",
    "web-vitals": "^5.1.0"
  }
}
